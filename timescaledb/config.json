{
  "name": "TimescaleDB",
  "version": "dev",
  "stage": "experimental",
  "slug": "timescaledb",
  "description": "An open-source database built on PostgreSQL for analyzing time-series data with the power and convenience of SQL",
  "url": "https://github.com/Expaso/hassos-addon-timescaledb",
  "arch": ["aarch64", "amd64"],
  "startup": "system",
  "boot": "auto",
  "init": false,
  "timeout": 20,
  "ports": {
    "5432/tcp": null
  },
  "options": {
    "databases": ["homeassistant"],
    "logins": [{ "username": "homeassistant", "password": null, "timescaledb": true }],
    "rights": [
      {
        "username": "homeassistant",
        "database": "homeassistant"
      }
	],
	"timescaledb": 
		{ 
      "telemetry": "basic",
      "maxmemory": "512MB",
      "maxcpus": "4"
		}
  },
  "schema": {
    "databases": ["str"],
    "logins": [{ "username": "str", "password": "str", "timescaledb": "bool" }],
	"rights": [{ "username": "str", "database": "str" }],
  "timescaledb": 
    {	"telemetry": "list(off|basic)",
      "maxmemory": "str?",
      "maxcpus": "int(1,)?"
    }
  },
  "imageaftercd": "husselhans/hassos-addon-timescaledb-{arch}"
}
