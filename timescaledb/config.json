{
  "name": "TimescaleDB",
  "version": "dev",
  "stage": "experimental",
  "slug": "timescaledb",
  "description": "An open-source database built on PostgreSQL for analyzing time-series data with the power and convenience of SQL",
  "url": "https://github.com/Expaso/hassos-addon-timescaledb",
  "arch": ["aarch64", "amd64", "i386", "armv7", "armhf"],
  "startup": "system",
  "boot": "auto",
  "init": false,
  "timeout": 20,
  "ports": {
    "5432/tcp": null
  },
  "options": {
    "databases": ["homeassistant"],
    "timescale_enabled": ["homeassistant"],
    
    "timescaledb": { 
      "telemetry": "basic",
      "maxmemory": "512MB",
      "maxcpus": "4"
      }
  },
  "schema": {
    "databases": ["str"],
    "timescale_enabled": ["str"],
    "timescaledb": {	
      "telemetry": "list(off|basic)",
      "maxmemory": "str?",
      "maxcpus": "int(1,)?"
    }
  },
  "imageaftercd": "husselhans/hassos-addon-timescaledb-{arch}"
}
